<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>JandaBox</id>
    <version>$Version$</version>
    <title>JandaBox</title>
    <authors>Matt Janda</authors>
    <owners>Matt Janda</owners>
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>dotnet template jandabox</tags>
    <description>Provides solution template for console application with dependency injection, logging, and configuration. 
Run `dotnet new install jandabox` to install the templates.</description>
    <projectUrl>https://github.com/Jandini/JandaBox</projectUrl>
    <repository type="git" url="https://github.com/Jandini/JandaBox" />
    <icon>images\icon.png</icon>
  </metadata>
  <files>
    <file src="src\**\*" target="content\src" exclude="src\**\obj\**;src\**\.vs\**;src\**\bin\**" />
    <file src="box.png" target="images\icon.png" />
  </files>
</package>
